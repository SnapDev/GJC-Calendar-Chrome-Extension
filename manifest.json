{
    "name":"GJC Calendar powered by SnapDev",
    "version":"2.3",
    "manifest_version":2,
    "minimum_chrome_version":"37.0.0.0",
    "permissions":["webview","power","storage","geolocation","pointerLock","system.display",{
      "fileSystem":["write","retainEntries","directory"]
    },"accessibilityFeatures.read","accessibilityFeatures.modify"],
